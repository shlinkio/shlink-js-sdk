{
  "name": "@shlinkio/shlink-js-sdk",
  "description": "A javascript Shlink's REST API client for the browser and node.js",
  "homepage": "https://shlink.io",
  "author": "Alejandro Celaya <alejandro@alejandrocelaya.com>",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./api-contract": {
      "types": "./dist/api-contract.d.ts",
      "import": "./dist/api-contract.js"
    },
    "./browser": {
      "types": "./dist/browser.d.ts",
      "import": "./dist/browser.js"
    },
    "./node": {
      "types": "./dist/node.d.ts",
      "import": "./dist/node.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "test": "vitest run",
    "test:integration": "test/run-integration-tests.sh",
    "test:ci": "npm run test -- --coverage",
    "lint": "eslint src test",
    "types": "tsc"
  },
  "devDependencies": {
    "@shlinkio/eslint-config-js-coding-standard": "~3.3.0",
    "@stylistic/eslint-plugin": "^3.1.0",
    "@total-typescript/shoehorn": "^0.1.2",
    "@vitest/coverage-v8": "^3.0.5",
    "eslint": "^9.20.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.4",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "jsdom": "^26.0.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.23.0",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.2"
  }
}
